{
  "expo": {
    "name": "DevizCar",
    "slug": "devizcar",
    "version": "1.0.2",
    "icon": "./assets/icon1.png",
    "userInterfaceStyle": "dark",
    "scheme": "devizcar",
    "jsEngine": "hermes",
    "plugins": [
      "@react-native-google-signin/google-signin",
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to set on icon App."
        }
      ],
      "expo-localization"
    ],
    "splash": {
      "image": "./assets/mySplash.png",
      "resizeMode": "cover",
      "backgroundColor": "#000001"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "splash": {
        "image": "./assets/mySplash.png",
        "resizeMode": "cover",
        "backgroundColor": "#000001"
      },
      "googleServicesFile": "./google-services.json",
      "icon": "./assets/icon1.png",
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon_adapt.png"
      },
      "package": "com.deviz.devizcar",
      "permissions": [
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.READ_EXTERNAL_STORAGE"
      ],
      "versionCode": 8
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "e2b0faf4-4cf0-48e6-af81-ece0785f388f"
      }
    },
    "owner": "aleksiz35"
  }
}
