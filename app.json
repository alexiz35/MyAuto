{
  "expo": {
    "name": "DevizCar",
    "slug": "DevizCar",
    "version": "1.0.0",
    "icon": "./assets/icon2.png",
    "userInterfaceStyle": "dark",
    "scheme": "devizcar",
    "jsEngine": "hermes",
    "plugins": [
      "@react-native-google-signin/google-signin",
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to set on icon App."
        }
      ]

    ],
    "splash": {
      "image": "./assets/mySplash.png",
      "resizeMode": "cover",
      "backgroundColor": "#000000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon7.png"

      },
      "package": "com.deviz.devizcar",
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION",
        "FOREGROUND_SERVICE",
        "ACCESS_BACKGROUND_LOCATION",
        "WRITE_EXTERNAL_STORAGE",
        "READ_EXTERNAL_STORAGE"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "872349f7-1492-4da4-b5ec-4b501afa5508"
      }
    }
  }
}
